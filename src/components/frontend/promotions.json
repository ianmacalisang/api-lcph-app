{
  "collectionName": "components_frontend_promotions",
  "info": {
    "displayName": "Promotions",
    "description": ""
  },
  "options": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "pricing": {
      "type": "decimal"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "validity": {
      "type": "date"
    },
    "courses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course.course"
    },
    "books": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::book.book"
    },
    "downloads": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::download.download"
    },
    "duration": {
      "type": "integer",
      "default": 12
    }
  }
